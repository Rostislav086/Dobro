         <div class=\"row justify-content-between\">\n                        <div class=\"col-6\">\n                            <a href=\"#\" class=\"btn btn-sm btn-success\">Выбрать</a>\n                        </div>\n                        <div class=\"col-6 text-right\">\n                            <a href=\"#\" class=\"btn btn-sm btn-success disabled\">Подробнее</a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'FondsList',\n  props: ['category'],\n  data() {\n    return {\n      fonds: null,\n    };\n  },\n  methods: {\n    async getFonds() {\n      if (this.category) {\n        const response = await axios.get(`/api/fonds/${this.category.id}`);\n        this.fonds = response.data;\n      } else {\n        const response = await axios.get('/api/fonds/');\n        this.fonds = response.data;\n        // console.log(this.fonds);\n      }\n    },\n  },\n  mounted() {\n    this.getFonds();\n  },\n};\n\n</script>\n\n<style lang=\"sass\" scoped>\n.card\n    height: 100%\n</style>\n"]}]}